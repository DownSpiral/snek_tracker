{
  "name": "Snek Tracker",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Connects to a socket.io server and shares your coords in slither.io",
  "content_scripts": [ {
    "all_frames": true,
    "js": [ "socket.io.min.js", "inject.js" ],
    "matches": [ "http://slither.io/" ]
  } ],
  "web_accessible_resources": [
    "socket.io.min.js",
    "content.js"
  ]
}
